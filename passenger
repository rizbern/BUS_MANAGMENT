#include<iostream>
using namespace std;
class Passenger{
protected: 
char fname[10];
char lname[10];
int age;
public:
void getdata(){
  cout<<"Enter your first name";
  cin>>fname;
  cout<<"Enter your last name";
  cin>>lname;
  cout<<"Enter your age";
  cin>>age;
}
void displayData(){
  cout<<fname<<" "<<lname<<endl;
  cout<<age;
}
};
